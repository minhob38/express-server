{
  "name": "express-server",
  "version": "1.0.0",
  "description": "basic express server",
  "main": "server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "server": "node -r module-alias/register dist/src/server.js",
    "docker:build": "docker build -t express-server:latest .",
    "docker:push": "docker tag express-server:latest minhob38/express-server:latest && docker push minhob38/express-server:latest",
    "docker:compose-up": "docker-compose up -d",
    "docker:compose-down": "docker-compose down",
    "docker:db-up": "docker-compose -f docker-database/docker-compose.database.yml --env-file .env up -d",
    "docker:db-down": "docker-compose -f docker-database/docker-compose.database.yml --env-file .env down",
    "test": "jest"
  },
  "author": "kwon, minho",
  "license": "ISC",
  "dependencies": {
    "@types/http-errors": "^1.8.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.14",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "pug": "^3.0.2",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.7",
    "jest-extended": "^1.2.0",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "_moduleAliases": {
    "@src": "dist/src"
  }
}
